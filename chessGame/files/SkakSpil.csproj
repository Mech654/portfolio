    <Resource Include="Assets\ChessP\Team White\knight_white.png" />
    <Resource Include="Assets\ChessP\Team White\pawn_white.png" />
    <Resource Include="Assets\ChessP\Team White\queen_white.png" />
    <Resource Include="Assets\ChessP\Team White\rook_white.png" />
